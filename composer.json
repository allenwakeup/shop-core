{
    "name": "goodcatch/laravel-module-core",
    "description": "goodcatch laravel module - core, the most practically and basic module for modules",
    "authors": [
        {
            "name": "Allen Li",
            "email": "ali@goodcatch.cn"
        }
    ],
    "extra": {
        "nexus-push": {
            "url": "https://repos.goodcatch.cn/repository/composer",
            "ignore-by-git-attributes": true,
            "ignore": [
                "test.php",
                "auth.json",
                ".git",
                ".DS_Store",
                ".idea/",
                "vendor/"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Goodcatch\\Modules\\Core\\": "src"
        },
        "classmap": [
            "database/seeds"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "illuminate/support": "^6.0",
        "laravel/framework": "^6.0",
        "elendev/nexus-composer-push": "^0.2.2",
        "laravel/helpers": "^1.1",
        "laravel/laravel": "^6.0",
        "mcamara/laravel-localization": "^1.5",
        "goodcatch/laravel-modules": "dev-master"
    }
}
